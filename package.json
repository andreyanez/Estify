{
	"name": "estify",
	"version": "1.0.0",
	"description": "app for visualizing spotify data",
	"main": "index.js",
	"engines": {
		"node": "16.17.0"
	},
	"scripts": {
		"dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
		"server": "nodemon index.js",
		"client": "cd client && npm run dev",
		"build": "NODE_ENV=production cd client/ && npm install && npm run build"
	},
	"keywords": [
		"spotify"
	],
	"author": "Andre Yanez",
	"license": "ISC",
	"devDependencies": {
		"@types/node": "^18.11.9",
		"nodemon": "^2.0.20"
	},
	"dependencies": {
		"axios": "^1.1.3",
		"concurrently": "^7.6.0",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"query-string": "^7.1.1"
	}
}
